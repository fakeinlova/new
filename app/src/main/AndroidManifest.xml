<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.alpha2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Alpha2"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".registration.LgRg"
            android:exported="false" />
        <activity
            android:name=".MainSceen"
            android:exported="false"
            android:label="@string/title_activity_main_sceen"
            android:theme="@style/Theme.Alpha2.NoActionBar" />
        <activity
            android:name=".registration.LogAndReg"
            android:exported="false" />
        <activity
            android:name=".Scroll"
            android:exported="false" />
        <activity
            android:name=".SplashSceene"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>